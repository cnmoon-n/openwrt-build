
config wifi-device 'radio2'
	option type 'mac80211'
	option path '1e140000.pcie/pci0000:00/0000:00:01.0/0000:02:00.0'
	option channel '1'
	option band '2g'
	option htmode 'HT20'
	option country 'US'
	option cell_density '0'
	option mu_beamformer '0'

config wifi-iface 'default_radio2'
	option device 'radio2'
	option network 'lan'
	option mode 'ap'
	option ssid 'J.HOME'
	option key 'jx201102'
	option ieee80211r '1'
	option mobility_domain '1100'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option r0_key_lifetime '20000'
	option encryption 'psk2+ccmp'

config wifi-device 'radio3'
	option type 'mac80211'
	option path '1e140000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'
	option channel '36'
	option band '5g'
	option htmode 'VHT80'
	option country 'US'
	option cell_density '0'
	option mu_beamformer '0'

config wifi-iface 'default_radio3'
	option device 'radio3'
	option network 'lan'
	option mode 'ap'
	option ssid 'J.HOME_5G'
	option key 'jx201102'
	option ieee80211r '1'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option reassociation_deadline '20000'
	option encryption 'psk2+ccmp'
	option mobility_domain '1111'

