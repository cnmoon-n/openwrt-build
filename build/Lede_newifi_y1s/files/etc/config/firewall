
config rule
	option family 'ipv6'
	option name 'note 10'
	option proto 'tcp udp'
	option src 'lan'
	option src_mac '88:F8:72:C6:04:AE'
	option target 'REJECT'

config rule
	option name 'note10 ipv6 ICMP'
	option proto 'icmp'
	option src 'lan'
	option src_mac '88:F8:72:C6:04:AE'
	option target 'REJECT'
	option family 'ipv6'

config rule
	option src 'lan'
	option target 'REJECT'
	option name 'hasense-wifi to route'
	option proto 'all'
	option src_mac 'D4:B7:61:99:BD:0B'

config defaults
	option syn_flood '1'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option fullcone '1'
	option flow_offloading '1'
	option flow_offloading_hw '1'

config zone
	option name 'lan'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option network 'lan'

config include
	option path '/etc/firewall.user'

config zone
	option input 'ACCEPT'
	option output 'ACCEPT'
	option name 'vpn_server'
	option forward 'ACCEPT'
	option masq '1'
	option network 'vpn_server VPN_SERVER'

config forwarding
	option dest 'vpn'
	option src 'vpn_server'

config forwarding
	option dest 'vpn_server'
	option src 'vpn'

config forwarding
	option dest 'vpn'
	option src 'lan'

config forwarding
	option dest 'vpn_server'
	option src 'lan'

config forwarding
	option dest 'lan'
	option src 'vpn'

config forwarding
	option dest 'lan'
	option src 'vpn_server'

config include 'zerotier'
	option type 'script'
	option path '/etc/zerotier.start'
	option reload '1'

config include 'mia'
	option type 'script'
	option path '/etc/mia.include'
	option reload '1'

config include 'openclash'
	option type 'script'
	option path '/var/etc/openclash.include'
	option reload '1'

config include 'unblockmusic'
	option type 'script'
	option path '/var/etc/unblockmusic.include'
	option reload '1'

config include 'miniupnpd'
	option type 'script'
	option path '/usr/share/miniupnpd/firewall.include'
	option family 'any'
	option reload '1'

config zone 'vpn'
	option name 'vpn'
	option input 'ACCEPT'
	option forward 'ACCEPT'
	option output 'ACCEPT'
	option masq '1'
	option network 'vpn0 vpn'

config forwarding 'vpntolan'
	option src 'vpn'
	option dest 'lan'

config forwarding 'lantovpn'
	option src 'lan'
	option dest 'vpn'

config include 'v2ray_server'
	option type 'script'
	option path '/var/etc/v2ray_server.include'
	option reload '1'

