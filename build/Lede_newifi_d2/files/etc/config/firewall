config include 'shadowsocksr'
	option type 'script'
	option path '/var/etc/shadowsocksr.include'
	option reload '1'

config rule 'kms'
	option name 'kms'
	option target 'ACCEPT'
	option src 'wan'
	option proto 'tcp'
	option dest_port '1688'

config rule
	option proto 'tcp'
	option src 'wan'
	option target 'ACCEPT'
	option family 'ipv6'
	option name 'lan ssh'
	option dest 'lan'
	option dest_port '22'

config rule
	option proto 'tcp'
	option dest_port '443'
	option name 'route'
	option target 'ACCEPT'
	option src 'wan'

