	option proto 'icmp'
	option src 'lan'
	option src_mac '88:F8:72:C6:04:AE'
	option target 'REJECT'
	option family 'ipv6'

config rule
	option src 'lan'
	option dest 'wan'
	option target 'REJECT'
	option proto 'all'
	option name 'hasense-wifi'
	option src_mac 'D4:B7:61:99:BD:0B'

config rule
	option src 'lan'
	option target 'REJECT'
	option name 'hasense-wifi to route'
	option proto 'all'
	option src_mac 'D4:B7:61:99:BD:0B'
	option enabled '0'

config rule
	option target 'ACCEPT'
	option name 'hasense-Lan_1900-2000'
	option src 'lan'
	option dest 'wan'
	option start_time '19:00:00'
	option stop_time '20:00:00'
	option family 'ipv4'
	option proto 'all'
	option src_mac '08:BA:5F:AD:8D:DB'
	option enabled '0'

