
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option packet_steering '1'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'bond-lan'
	list ports 'lan1'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option netmask '255.255.255.0'
	option ip6assign '64'
	option ipaddr '192.168.8.1'

config device
	option name 'wan'
	option macaddr 'a4:39:b3:23:fd:dc'

config interface 'wan'
	option device 'wan'
	option proto 'dhcp'

config interface 'wan6'
	option device 'wan'
	option proto 'dhcpv6'

config interface 'tun1'
	option proto 'none'
	option device 'tun1'

config interface 'ovpn_server'
	option proto 'none'
	option device 'tun2'

config interface 'vpn0'
	option proto 'none'
	option device 'tun0'

